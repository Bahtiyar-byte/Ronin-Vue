{
  "name": "evanscrm",
  "description": "Evans CRM - template backend",
  "scripts": {
    "start": "npm run db:migrate && npm run db:seed && nodemon ./src/index.js",
    "dev": "nodemon ./src/index.js localhost 8080",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "db:drop": "sequelize-cli db:drop",
    "db:create": "sequelize-cli db:create"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.18.2",
    "axios": "^1.7.2",
    "bcrypt": "5.1.1",
    "cors": "2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "express": "4.19.2",
    "formidable": "1.2.2",
    "helmet": "7.1.0",
    "json2csv": "^5.0.7",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "multer": "^1.4.4",
    "mysql2": "2.2.5",
    "nodemailer": "6.9.14",
    "passport": "^0.7.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "passport-microsoft": "^2.1.0",
    "pg": "8.12.0",
    "pg-hstore": "2.3.4",
    "sequelize": "6.35.2",
    "sequelize-json-schema": "^2.1.1",
    "sqlite": "4.0.15",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tedious": "^18.2.1",
    "ws": "^8.17.1"
  },
  "engines": {
    "node": ">=20"
  },
  "private": true,
  "devDependencies": {
    "cross-env": "7.0.3",
    "mocha": "10.5.2",
    "node-mocks-http": "1.9.0",
    "nodemon": "3.1.4",
    "sequelize-cli": "6.6.2"
  }
}
